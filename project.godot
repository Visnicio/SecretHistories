; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Secret Histories"
config/description="An entirely forgettable game"
run/main_scene="res://scenes/ui/opening_screens.tscn"
config/features=PackedStringArray("4.3")
run/flush_stdout_on_print=true
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

GameManager="*res://globals/game_manager.gd"
KeybindingManager="*res://globals/keybinding_manager.gd"
GameSettings="*res://globals/settings/game_settings.gd"
InputSettings="*res://globals/settings/input_settings.gd"
AudioSettings="*res://globals/settings/audio_settings.gd"
VideoSettings="*res://globals/settings/video_settings.gd"
KeyInputSettings="*res://globals/settings/key_input_settings.gd"
Save="*res://globals/save.gd"
LoadQuotes="*res://globals/load_quotes.gd"
Settings="*res://globals/settings.gd"
LoadScene="*res://globals/load_scene.gd"
BackgroundMusic="*res://scenes/effects/background_music.tscn"
SettingsConfig="*res://globals/settings_config.gd"
Events="*res://globals/events.gd"
ImGuiRoot="*res://addons/imgui-godot/data/ImGuiRoot.tscn"

[debug]

gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/size/fullscreen=true

[editor]

description="An entirely forgettable game"

[editor_plugins]

enabled=PackedStringArray("res://addons/imgui-godot/plugin.cfg")

